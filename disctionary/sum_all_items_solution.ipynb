   "source": [
    "# Sum all the items in a dictionary\n",
    "# Input {'data1':100,'data2':-54,'data3':247}\n",
    "# Output 293\n",
    "\n",
    "def sumOfDictItems(dict):\n",
    "    sum = 0\n",
    "    for value in dict.values():\n",
    "        sum += value\n",
    "    return sum\n",
    "\n",
    "\n",
    "sumOfDictItems({\n",
    "    1: 3,\n",
    "    2: 33,\n",
    "    4: 333,\n",
    "})"
   ]
  }
